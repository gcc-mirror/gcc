@opindex fdump-rtl-dce1
@opindex fdump-rtl-dce2
@item -fdump-rtl-dce1
@itemx -fdump-rtl-dce2
@option{-fdump-rtl-dce1} and @option{-fdump-rtl-dce2} enable dumping after

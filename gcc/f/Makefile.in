# These exist for maintenance purposes.

# Update the tags table.
TAGS: force
	cd $(srcdir)/f ;			\
	etags *.c *.h ;				\
	echo 'l' | tr 'l' '\f' >> TAGS ;	\
	echo 'parse.y,0' >> TAGS ; 		\
	etags -a ../*.h ../*.c;

.PHONY: none all all.indirect f77.rebuilt compiler native deps-kinda TAGS g77-only

force:

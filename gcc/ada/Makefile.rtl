  # Extra pairs for the vthreads runtime
  ifeq ($(strip $(filter-out vthreads,$(THREAD_KIND))),)
    LIBGNAT_TARGET_PAIRS += \
    s-thread.adb<libgnat/s-thread__ae653.adb \
    s-osvers.ads<libgnat/s-osvers__vxworks-653.ads \

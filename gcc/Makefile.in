  insn-config.h insn-flags.h insn-codes.h system.h reload.h $(RECOG_H) \
  function.h flags.h hard-reg-set.h resource.h
	$(CC) $(ALL_CFLAGS) $(ALL_CPPFLAGS) $(INCLUDES) -c insn-emit.c


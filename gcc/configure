#  endif /* no HAVE_SYS_PARAM_H */
# endif /* no _SC_PAGESIZE */

#endif /* no HAVE_GETPAGESIZE */

int main()
{
  char *x;
  int fd, pg;

  fd = open("/dev/zero", O_RDWR);
  if (fd < 0)
    exit(1);

  pg = getpagesize();
  x = (char*)mmap(0, pg, PROT_READ|PROT_WRITE, MAP_PRIVATE, fd, 0);
  if (x == (char *) -1)
    exit(2);

  *(int *)x += 1;

  if (munmap(x, pg) < 0)
    exit(3);

  exit(0);
}

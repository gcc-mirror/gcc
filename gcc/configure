/* end confdefs.h.  */
#include <limits.h>
	       #ifndef LLONG_MAX
	       # define HALF \
			(1LL << (sizeof (long long int) * CHAR_BIT - 2))
	       # define LLONG_MAX (HALF - 1 + HALF)
	       #endif
int
main ()
{
long long int n = 1;
	       int i;
	       for (i = 0; ; i++)
		 {
		   long long int m = n << i;
		   if (m >> i != n)
		     return 1;
		   if (LLONG_MAX / 2 < m)
		     break;
		 }
	       return 0;
  ;
  return 0;
}

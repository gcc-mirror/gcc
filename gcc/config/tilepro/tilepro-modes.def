/* Extra modes for handling struct returns in up to 10 registers. */
INT_MODE (R3I, 12);
INT_MODE (R5I, 20);
INT_MODE (R6I, 24);
INT_MODE (R7I, 28);
INT_MODE (R8I, 32);
INT_MODE (R9I, 36);
INT_MODE (R10I, 40);

/* Vector modes.  */
VECTOR_MODES (INT, 4);    /* V4QI V2HI */
VECTOR_MODE (INT, QI, 8); /* V8QI */
VECTOR_MODE (INT, HI, 4); /* V4HI */
VECTOR_MODE (INT, QI, 2); /* V2QI */

generic
     type integer_type is range <>;
procedure LA14019_1 (Test_val : in out integer);

with LA14019_0;
procedure LA14019_1 (Test_val : in out integer) is
     arr : array (1..5) of integer;
     sum : integer := 0;
     temp_val : integer := 0;
begin
     arr(1) := Test_val;
     for i in 2..arr'last loop
          temp_val := arr(i-1);
          LA14019_0 (temp_val);
          arr(i) := temp_val;
     end loop;
     for i in 1..arr'last loop
          sum := sum + arr(i);
     end loop;
     Test_val := sum;
end LA14019_1;

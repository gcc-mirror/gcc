# used.
SET_GCC_LIB_PATH=
if test -d ${srcdir}/gcc && test x${is_cross_compiler} = xno; then
  case "${set_lib_path}" in
    no) ;;
    *)
      eval "d=\$$RPATH_ENVVAR"
      if test x"$d" != x; then
	d="$pwd/gcc:$d"
      else
	d="$pwd/gcc"
    fi
    SET_GCC_LIB_PATH="\$(RPATH_ENVVAR)=$d; export \$(RPATH_ENVVAR);"
    ;;
  esac
fi



  template<typename _Tp0, typename _Tp1, typename _Tp2, typename _Tp3,
	   typename _Tp4, typename _Tp5, typename _Tp6>
    class tuple<_Tp0, _Tp1, _Tp2, _Tp3, _Tp4, _Tp5, _Tp6, _NullClass,
	        _NullClass, _NullClass>
    {
      _Tp0 _M_t0;
      _Tp1 _M_t1;
      _Tp2 _M_t2;
      _Tp3 _M_t3;
      _Tp4 _M_t4;
      _Tp5 _M_t5;
      _Tp6 _M_t6;


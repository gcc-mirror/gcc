        friend class tuple;
  };

  template<typename _Tp0, typename _Tp1, typename _Tp2, typename _Tp3,
	   typename _Tp4, typename _Tp5, typename _Tp6, typename _Tp7,
	   typename _Tp8>
    class tuple<_Tp0, _Tp1, _Tp2, _Tp3, _Tp4, _Tp5, _Tp6, _Tp7, _Tp8, 

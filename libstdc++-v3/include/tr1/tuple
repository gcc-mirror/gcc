        _M_t0(__in._M_t0), _M_t1(__in._M_t1), _M_t2(__in._M_t2),
        _M_t3(__in._M_t3), _M_t4(__in._M_t4), _M_t5(__in._M_t5),
        _M_t6(__in._M_t6)
      { }


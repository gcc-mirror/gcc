    $as_echo "#define HAVE_FABSL 1" >>confdefs.h

    $as_echo "#define HAVE_FLOORL 1" >>confdefs.h

    $as_echo "#define HAVE_FMODL 1" >>confdefs.h

    $as_echo "#define HAVE_FREXPL 1" >>confdefs.h


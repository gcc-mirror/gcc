  *[\\\"\#\$\&\'\`$am_lf]*)
    as_fn_error "unsafe absolute working directory name" "$LINENO" 5;;
esac
case $srcdir in
  *[\\\"\#\$\&\'\`$am_lf\ \	]*)
    as_fn_error "unsafe srcdir value: \`$srcdir'" "$LINENO" 5;;

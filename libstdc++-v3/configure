      toplevel_srcdir="\${top_srcdir}/${with_multisrctop}../.."
      auxdir="${srcdir}/${with_multisrctop}../.."
    else
      toplevel_srcdir="\${top_srcdir}/${with_multisrctop}.."
      auxdir="${srcdir}/${with_multisrctop}.."
    fi
  fi
else
  toplevel_srcdir="\${top_srcdir}/.."
